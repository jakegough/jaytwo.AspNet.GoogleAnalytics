    <authors>Jake Gough</authors>
    <projectUrl>https://github.com/jakegough/jaytwo.AspNet.GoogleAnalytics/</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/jakegough/jaytwo.AspNet.GoogleAnalytics/master/LICENSE</licenseUrl>
